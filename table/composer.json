{
	"name": "modules/table",
	"description": "InertiaUI Table Module",
	"type": "library",
	"version": "1.0",
	"license": "proprietary",
	"require": {},
	"suggest": {
		"kirschbaum-development/eloquent-power-joins": "To order tables by related models. (3.5 or ^4.0)",
		"maatwebsite/excel": "To export tables to CSV, Excel, etc. (^3.1)"
	},
	"autoload": {
		"psr-4": {
			"Modules\\Table\\": "src/",
			"Modules\\Table\\Tests\\": "tests/",
			"Modules\\Table\\Database\\Factories\\": "database/factories/",
			"Modules\\Table\\Database\\Seeders\\": "database/seeders/"
		}
	},
	"minimum-stability": "stable",
	"extra": {
		"laravel": {
			"providers": [
				"Modules\\Table\\Providers\\TableServiceProvider"
			]
		}
	}
}
